<Window x:Class="Pomodole.ConfigWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Pomodole"
        mc:Ignorable="d"
        Title="Config" Height="250" Width="350" Name="ConfigMainWindow">
    <StackPanel>
        <Grid x:Name="ConfigWindowMainGrid" HorizontalAlignment="Center" VerticalAlignment="Top" Width="{Binding Width, ElementName=ConfigMainWindow}" Height="175" Margin="-4,0">
            

        </Grid>
        <Button x:Name="button" Content="OK" HorizontalAlignment="Center" Height="21" Margin="0,0,0,0" VerticalAlignment="Center" Width="74" Command="New"/>
    </StackPanel>
</Window>
